    "log-timestamp": "0.2.1",
    "nodemon": "1.18.4",
    "properties-reader": "0.0.16",
    "sinon": "^13.0.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "mocha": "8.4.0",
